@model IEnumerable<MusicLibrary.Models.PlaylistSong>

@{
    ViewData["Title"] = "See your music";
}

<h1>Music from your playlist</h1>

<h4>Songs</h4>
<hr />
<<table class="table table-striped table-hover;text-primary">
    <thead>
        <tr>
            <th>
                Music element
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                 <a asp-controller="Songs" asp-action="Details" asp-route-id="@item.SongId">
                       Your song Music
                 </a>
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
