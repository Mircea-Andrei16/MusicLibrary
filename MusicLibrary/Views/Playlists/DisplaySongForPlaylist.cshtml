@model IEnumerable<MusicLibrary.Models.PlaylistSong>

@{
    ViewData["Title"] = "See your music";
}

<h1>Music from your playlist</h1>

<table class="table table-striped table-hover;text-primary">
    <thead>
        <tr>
            <th>
                Music element
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                 <a class="btn btn-outline-primary" asp-controller="Songs" asp-action="Details" asp-route-id="@item.SongId">
                       Song
                 </a>
            </td>
        </tr>
}
    </tbody>
</table>
<div>
    <button class="button button2" onclick="history.back()">Go Back</button>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
